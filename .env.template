# Azure KeyVault Configuration
# Copy this file to .env and fill in your values
# DO NOT commit .env to source control

# Globally unique name for your KeyVault (3-24 chars, alphanumeric and hyphens only)
VAULT_NAME=my-keyvault-name

# Resource group to create/use for the vault
RESOURCE_GROUP_NAME=my-resource-group

# Azure subscription ID (leave empty to use your default subscription)
SUBSCRIPTION_ID=
